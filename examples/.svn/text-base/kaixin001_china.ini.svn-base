# Network Dynamics of Mobile Social Networks - ICC 2014

[General]
number_of_users = 3980
number_of_files = 10000
mapping_algorithm = "random"
time_limit = 432000
social_graph = facebook
communities_enabled = true
number_of_communities = 5

[InterSessionTime]
# FROM THE PAPER
distribution = lognorm
param1 = 1.97
param2 = 1.12

[InterActivityTime]
# EXTRACTED FROM THE PAPER
distribution = lognorm
param1 = 1.27
param2 = 0.85

[SessionLength]
# EXTRACTED FROM THE PAPER
distribution = zipf
param1 = 1.529
param2 = 3.840

[SessionsPerUser]
# FROM THE PAPER
distribution = zipf
param1 = 1.5
param2 = 3.01

[MarkovChain]
file = orkut.gml

[SelectContent]
alpha=0.65

[Mobility]
model_name = random_waypoint
x_size = 100
y_size = 100
